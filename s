[32m+[m[32m            'queue' => env('REDIS_QUEUE', 'default'),[m
[32m+[m[32m            'retry_after' => (int) env('REDIS_QUEUE_RETRY_AFTER', 90),[m
[32m+[m[32m            'block_for' => null,[m
[32m+[m[32m            'after_commit' => false,[m
