[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mnamespace App\Filament\Resources;[m
[32m+[m
[32m+[m[32muse App\Filament\Resources\ContactResource\Pages;[m
[32m+[m[32muse App\Filament\Resources\ContactResource\RelationManagers;[m
[32m+[m[32muse App\Filament\Widgets\MapWidget;[m
[32m+[m[32muse App\Models\Contact;[m
[32m+[m[32muse App\Services\LocationService;[m
[32m+[m[32muse Filament\Forms;[m
[32m+[m[32muse Filament\Forms\Components\Hidden;[m
[32m+[m[32muse Filament\Forms\Components\TextInput;[m
[32m+[m[32muse Filament\Forms\Form;[m
[32m+[m[32muse Filament\Resources\Resource;[m
[32m+[m[32muse Filament\Tables;[m
[32m+[m[32muse Filament\Tables\Columns\TextColumn;[m
[32m+[m[32muse Filament\Tables\Table;[m
[32m+[m[32muse Illuminate\Database\Eloquent\Builder;[m
[32m+[m[32muse Illuminate\Database\Eloquent\SoftDeletingScope;[m
[32m+[m
[32m+[m[32mclass ContactResource extends Resource[m
[32m+[m[32m{[m
[32m+[m[32m    protected static ?string $model = Contact::class;[m
[32m+[m
[32m+[m[32m    protected static ?string $navigationIcon = 'heroicon-o-phone';[m
[32m+[m
[32m+[m[32m    public static function form(Form $form): Form[m
[32m+[m[32m    {[m
[32m+[m[32m        return $form[m
[32m+[m[32m            ->schema([[m
[32m+[m[32m                TextInput::make('email')->email()[m
[32m+[m[32m                    ->hintIcon('heroicon-o-envelope')[m
[32m+[m[32m                    ->placeholder('enter your email address')[m
[32m+[m[32m                    ->required(),[m
[32m+[m[32m                TextInput::make('phone')->tel()->required()[m
[32m+[m[32m                    ->hintIcon('heroicon-o-phone'),[m
[32m+[m[32m                TextInput::make('location')[m
[32m+[m[32m                    ->required()[m
[32m+[m[32m                    ->hint('Masukan alamat lengkap atau tempat')[m
[32m+[m[32m                    ->hintIcon('heroicon-o-map-pin')[m
[32m+[m[32m                    ->afterStateUpdated(function ($state, callable $set) {[m
[32m+[m[32m                        if ($state) {[m
[32m+[m[32m                            $nominatim = app(LocationService::class);[m
[32m+[m[32m                            $coordinates = $nominatim->getCoordinates($state);[m
[32m+[m
[32m+[m[32m                            $set('latitude', $coordinates['latitude']);[m
[32m+[m[32m                            $set('longitude', $coordinates['longitude']);[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }),[m
[32m+[m[32m                Hidden::make('latitude'),[m
[32m+[m[32m                Hidden::make('longitude'),[m
[32m+[m[32m            ]);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static function table(Table $table): Table[m
[32m+[m[32m    {[m
[32m+[m[32m        return $table[m
[32m+[m[32m            ->columns([[m
[32m+[m[32m                TextColumn::make('email')->label('Email'),[m
[32m+[m[32m                TextColumn::make('phone')->label('Telepon'),[m
[32m+[m[32m                TextColumn::make('location')->label('Location'),[m
[32m+[m[32m            ])[m
[32m+[m[32m            ->filters([[m
[32m+[m[32m                //[m
[32m+[m[32m            ])[m
[32m+[m[32m            ->actions([[m
[32m+[m[32m                Tables\Actions\EditAction::make()[m
[32m+[m[32m                    ->color('warning'),[m
[32m+[m[32m                Tables\Actions\DeleteAction::make()[m
[32m+[m[32m            ])[m
[32m+[m[32m            ->bulkActions([[m
[32m+[m[32m                Tables\Actions\BulkActionGroup::make([[m
[32m+[m[32m                    Tables\Actions\DeleteBulkAction::make(),[m
[32m+[m[32m                ]),[m
[32m+[m[32m            ]);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static function getRelations(): array[m
[32m+[m[32m    {[m
[32m+[m[32m        return [[m
[32m+[m[32m            //[m
[32m+[m[32m        ];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static function getPages(): array[m
[32m+[m[32m    {[m
[32m+[m[32m        return [[m
[32m+[m[32m            'index' => Pages\ListContacts::route('/'),[m
[32m+[m[32m            // 'create' => Pages\CreateContact::route('/create'),[m
[32m+[m[32m            'edit' => Pages\EditContact::route('/{record}/edit'),[m
[32m+[m[32m        ];[m
[32m+[m[32m    }[m
[32m+[m[32m    public static function canCreate(): bool[m
[32m+[m[32m    {[m
[32m+[m[32m        return !Contact::exists();[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
