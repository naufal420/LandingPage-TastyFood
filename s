[32m+[m[32muse Filament\Forms;[m
[32m+[m[32muse Filament\Forms\Form;[m
[32m+[m[32muse Filament\Resources\Resource;[m
[32m+[m[32muse Filament\Tables;[m
[32m+[m[32muse Filament\Tables\Columns\TextColumn;[m
[32m+[m[32muse Filament\Tables\Table;[m
[32m+[m[32muse Filament\Tables\Actions\Action;[m
[32m+[m[32muse Illuminate\Database\Eloquent\Builder;[m
[32m+[m[32muse Illuminate\Database\Eloquent\SoftDeletingScope;[m
[32m+[m
[32m+[m[32mclass ContactMessageResource extends Resource[m
[32m+[m[32m{[m
[32m+[m[32m    protected static ?string $model = ContactMessage::class;[m
[32m+[m
[32m+[m[32m    protected static ?string $navigationLabel = 'Contacts Message';[m
[32m+[m
[32m+[m[32m    protected static ?string $navigationIcon = 'heroicon-o-chat-bubble-left';[m
[32m+[m
[32m+[m[32m    public static function form(Form $form): Form[m
[32m+[m[32m    {[m
[32m+[m[32m        return $form[m
[32m+[m[32m            ->schema([[m
[32m+[m[32m                //[m
[32m+[m[32m            ]);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static function table(Table $table): Table[m
[32m+[m[32m    {[m
[32m+[m[32m        return $table[m
[32m+[m[32m            ->columns([[m
[32m+[m[32m                TextColumn::make('subject')[m
[32m+[m[32m                    ->label('Subject')[m
[32m+[m[32m                    ->sortable()[m
[32m+[m[32m                    ->searchable()[m
[32m+[m[32m                    ->wrap(),[m
[32m+[m[32m                TextColumn::make('name')[m
[32m+[m[32m                    ->label('Name')[m
[32m+[m[32m                    ->sortable()[m
[32m+[m[32m                    ->searchable(),[m
[32m+[m[32m                TextColumn::make('email')[m
[32m+[m[32m                    ->label('Email')[m
[32m+[m[32m                    ->url(fn($record) => 'https://mail.google.com/mail/?view=cm&fs=1&to=' . $record->email)[m
[32m+[m[32m                    ->color('primary')[m
[32m+[m[32m                    ->openUrlInNewTab()[m
[32m+[m[32m                    ->sortable()[m
[32m+[m[32m                    ->searchable(),[m
[32m+[m[32m                TextColumn::make('created_at')[m
[32m+[m[32m                    ->label('Date message')[m
[32m+[m[32m                    ->dateTime('d M Y, H:i'),[m
[32m+[m[32m                TextColumn::make('message')[m
[32m+[m[32m                    ->label('Message')[m
[32m+[m[32m                    ->sortable()[m
[32m+[m[32m                    ->searchable()[m
[32m+[m[32m                    ->limit(100)[m
[32m+[m[32m                    ->wrap()[m
[32m+[m[32m            ])[m
[32m+[m[32m            ->filters([])[m
[32m+[m[32m            ->actions([[m
[32m+[m[32m                Action::make('View message details')[m
[32m+[m[32m                    ->icon('heroicon-o-eye')[m
[32m+[m[32m                    ->color('warning')[m
[32m+[m[32m                    ->modalHeading(false)[m
[32m+[m[32m                    ->modalSubmitAction(false)[m
[32m+[m[32m                    ->modalCloseButton()[m
[32m+[m[32m                    ->modalContent(fn($record) => view('admin.contact-message-detail', [[m
[32m+[m[32m                        'record' => $record[m
[32m+[m[32m                    ])),[m
[32m+[m[32m                Tables\Actions\DeleteAction::make()[m
[32m+[m[32m            ])[m
[32m+[m[32m            ->defaultSort('created_at', 'desc')[m
[32m+[m[32m            ->bulkActions([[m
[32m+[m[32m                Tables\Actions\BulkActionGroup::make([[m
[32m+[m[32m                    Tables\Actions\DeleteBulkAction::make(),[m
[32m+[m[32m                ]),[m
[32m+[m[32m            ]);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static function getRelations(): array[m
[32m+[m[32m    {[m
[32m+[m[32m        return [[m
[32m+[m[32m            //[m
[32m+[m[32m        ];[m
[32m+[m[32m    }[m
[32m+[m
