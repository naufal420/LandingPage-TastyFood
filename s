[32m+[m[32m    | Supported: "smtp", "sendmail", "mailgun", "ses", "ses-v2",[m
[32m+[m[32m    |            "postmark", "resend", "log", "array",[m
[32m+[m[32m    |            "failover", "roundrobin"[m
[32m+[m[32m    |[m
[32m+[m[32m    */[m
[32m+[m
[32m+[m[32m    'mailers' => [[m
[32m+[m
[32m+[m[32m        'smtp' => [[m
[32m+[m[32m            'transport' => 'smtp',[m
