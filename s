[32m+[m
[32m+[m[32m    public static function form(Form $form): Form[m
[32m+[m[32m    {[m
[32m+[m[32m        return $form[m
[32m+[m[32m            ->schema([[m
[32m+[m[32m                //[m
[32m+[m[32m            ]);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static function table(Table $table): Table[m
[32m+[m[32m    {[m
[32m+[m[32m        return $table[m
[32m+[m[32m            ->columns([[m
[32m+[m[32m                TextColumn::make('name')[m
[32m+[m[32m                    ->label('Name')[m
[32m+[m[32m                    ->sortable()[m
[32m+[m[32m                    ->searchable(),[m
[32m+[m[32m                TextColumn::make('email')[m
[32m+[m[32m                    ->label('Email')[m
[32m+[m[32m                    ->url(fn($record) => 'https://mail.google.com/mail/?view=cm&fs=1&to=' . $record->email)[m
[32m+[m[32m                    ->color('primary')[m
[32m+[m[32m                    ->openUrlInNewTab()[m
[32m+[m[32m                    ->sortable()[m
[32m+[m[32m                    ->searchable(),[m
[32m+[m[32m                ImageColumn::make('image')[m
[32m+[m[32m                    ->label('Image')[m
[32m+[m[32m                    ->disk('public')[m
[32m+[m[32m                    ->circular()[m
[32m+[m[32m                    ->size(70),[m
[32m+[m[32m                TextColumn::make('rating')[m
[32m+[m[32m                    ->label('Rating')[m
[32m+[m[32m                    ->formatStateUsing(function ($state) {[m
[32m+[m[32m                        return str_repeat('⭐', $state); // tampilkan bintang sebanyak nilai rating[m
[32m+[m[32m                    })[m
[32m+[m[32m                    ->sortable(),[m
[32m+[m[32m                TextColumn::make('comment')[m
[32m+[m[32m                    ->label('Comentar')[m
[32m+[m[32m                    ->sortable()[m
[32m+[m[32m                    ->searchable()[m
[32m+[m[32m                    ->limit(40)[m
[32m+[m[32m                    ->wrap()[m
[32m+[m[32m            ])[m
[32m+[m[32m            ->filters([[m
[32m+[m[32m                //[m
[32m+[m[32m            ])[m
[32m+[m[32m            ->actions([[m
[32m+[m[32m                Tables\Actions\Action::make('viewComment')[m
[32m+[m[32m                    ->label('Lihat Komentar')[m
[32m+[m[32m                    ->icon('heroicon-o-eye')[m
[32m+[m[32m                    ->color('warning')[m
[32m+[m[32m                    ->modalHeading('Komentar Lengkap')[m
[32m+[m[32m                    ->modalDescription(fn($record) => 'Dari: ' . $record->name)[m
[32m+[m[32m                    ->modalContent(fn($record) => new HtmlString(nl2br(e($record->comment))))[m
[32m+[m[32m                    ->modalSubmitAction(false)[m
[32m+[m[32m                    ->modalCloseButton(),[m
[32m+[m[32m                Tables\Actions\DeleteAction::make()[m
[32m+[m[32m            ])[m
[32m+[m[32m            ->bulkActions([[m
[32m+[m[32m                Tables\Actions\BulkActionGroup::make([[m
[32m+[m[32m                    Tables\Actions\DeleteBulkAction::make(),[m
[32m+[m[32m                ]),[m
[32m+[m[32m            ]);[m
[32m+[m[32m    }[m
