[32m+[m[32m        parent::mount();[m
[32m+[m
[32m+[m[32m        // Tandai semua pesan sebagai telah dibaca[m
[32m+[m[32m        ContactMessage::where('is_read', false)->update(['is_read' => true]);[m
[32m+[m
[32m+[m[32m        // Refresh halaman untuk memperbarui badge[m
[32m+[m[32m        $this->dispatch('refresh-navigation-badge');[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
