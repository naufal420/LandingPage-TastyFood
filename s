[1m+++ b/app/Http/Controllers/testimonialsController.php[m
[36m@@ -0,0 +1,44 @@[m
[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mnamespace App\Http\Controllers;[m
[32m+[m
[32m+[m[32muse App\Models\testimonials;[m
[32m+[m[32muse Illuminate\Http\Request;[m
[32m+[m[32muse Illuminate\Support\Facades\Log;[m
[32m+[m[32muse Illuminate\Support\Facades\Storage;[m
[32m+[m
[32m+[m[32mclass testimonialsController extends Controller[m
[32m+[m[32m{[m
[32m+[m[32m    public function index()[m
[32m+[m[32m    {[m
[32m+[m[32m        $testimonials = testimonials::all();[m
[32m+[m[32m        return view('testimonials', compact('testimonials'));[m
[32m+[m[32m    }[m
[32m+[m[32m    public function store(Request $request)[m
[32m+[m[32m    {[m
[32m+[m[32m        $request->validate([[m
[32m+[m[32m            'name' => 'required|string|max:255',[m
[32m+[m[32m            'email' => 'required|email|max:255',[m
[32m+[m[32m            'image' => 'required|image|mimes:jpeg,png,jpg|max:10000',[m
[32m+[m[32m            'rating' => 'required|integer|min:1|max:5',[m
[32m+[m[32m            'comment' => 'required|string',[m
[32m+[m[32m        ]);[m
[32m+[m[32m        try {[m
[32m+[m[32m            $fileName = time() . '_' . $request->file('image')->getClientOriginalName();[m
[32m+[m[32m            $imagePath = $request->file('image')->storeAs('testimonials', $fileName, 'public');[m
[32m+[m
[32m+[m[32m            testimonials::create([[m
[32m+[m[32m                'name' => $request->name,[m
[32m+[m[32m                'email' => $request->email,[m
[32m+[m[32m                'image' => $imagePath,[m
[32m+[m[32m                'rating' => $request->rating,[m
[32m+[m[32m                'comment' => $request->comment,[m
[32m+[m[32m            ]);[m
[32m+[m
[32m+[m[32m            return redirect()->back()->with('success', 'Testimoni berhasil dikirim!');[m
[32m+[m[32m        } catch (\Exception $e) {[m
[32m+[m[32m            Log::error('Gagal menyimpan testimoni: ' . $e->getMessage());[m
[32m+[m[32m            return redirect()->back()->with('error', 'Terjadi kesalahan saat mengirim testimoni. Silakan coba lagi.');[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
