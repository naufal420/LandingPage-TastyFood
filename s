[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mnamespace Database\Factories;[m
[32m+[m
[32m+[m[32muse Illuminate\Database\Eloquent\Factories\Factory;[m
[32m+[m[32muse Illuminate\Support\Facades\Hash;[m
[32m+[m[32muse Illuminate\Support\Str;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * @extends \Illuminate\Database\Eloquent\Factories\Factory<\App\Models\User>[m
[32m+[m[32m */[m
[32m+[m[32mclass UserFactory extends Factory[m
[32m+[m[32m{[m
[32m+[m[32m    /**[m
[32m+[m[32m     * The current password being used by the factory.[m
[32m+[m[32m     */[m
[32m+[m[32m    protected static ?string $password;[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Define the model's default state.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return array<string, mixed>[m
[32m+[m[32m     */[m
[32m+[m[32m    public function definition(): array[m
[32m+[m[32m    {[m
[32m+[m[32m        return [[m
[32m+[m[32m            'name' => fake()->name(),[m
[32m+[m[32m            'email' => fake()->unique()->safeEmail(),[m
[32m+[m[32m            'email_verified_at' => now(),[m
[32m+[m[32m            'password' => static::$password ??= Hash::make('password'),[m
[32m+[m[32m            'remember_token' => Str::random(10),[m
[32m+[m[32m        ];[m
[32m+[m[32m    }[m
