[32m+[m[32m        <div id="map" style="height: 400px;" class="rounded-lg"></div>[m
[32m+[m
[32m+[m[32m        {{-- Import Leaflet JS --}}[m
[32m+[m[32m        <script src="https://unpkg.com/leaflet@1.9.4/dist/leaflet.js"></script>[m
[32m+[m
[32m+[m[32m        <script>[m
[32m+[m[32m            // Inisialisasi map[m
[32m+[m[32m            var map = L.map('map').setView([{{ $latitude }}, {{ $longitude }}], {{ $zoom }});[m
[32m+[m[41m            [m
[32m+[m[32m            // Tambahkan layer OpenStreetMap[m
[32m+[m[32m            L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {[m
[32m+[m[32m                attribution: '© OpenStreetMap contributors'[m
[32m+[m[32m            }).addTo(map);[m
[32m+[m[41m            [m
[32m+[m[32m            // Tambahkan marker[m
[32m+[m[32m            L.marker([{{ $latitude }}, {{ $longitude }}]).addTo(map);[m
[32m+[m[32m        </script>[m
[32m+[m[32m    </x-filament::card>[m
[32m+[m[32m</x-filament::widget>[m
\ No newline at end of file[m
