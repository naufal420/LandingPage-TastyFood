[32m+[m[32m                Select::make('type')[m
[32m+[m[32m                    ->label('Type Gallery')[m
[32m+[m[32m                    ->options([[m
[32m+[m[32m                        'gallery' => 'Gallery',[m
[32m+[m[32m                        'gallery-slider' => 'Gallery Slider'[m
[32m+[m[32m                    ])[m
[32m+[m[32m                    ->default('gallery')[m
[32m+[m[32m                    ->required(),[m
