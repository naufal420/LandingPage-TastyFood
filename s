[32m+[m[32m    |[m
[32m+[m[32m    */[m
[32m+[m
[32m+[m[32m    'default' => env('LOG_CHANNEL', 'stack'),[m
[32m+[m
[32m+[m[32m    /*[m
[32m+[m[32m    |--------------------------------------------------------------------------[m
[32m+[m[32m    | Deprecations Log Channel[m
[32m+[m[32m    |--------------------------------------------------------------------------[m
[32m+[m[32m    |[m
