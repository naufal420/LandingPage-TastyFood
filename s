[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mnamespace App\Filament\ContactResource\Widgets;[m
[32m+[m
[32m+[m[32muse Filament\Widgets\Widget;[m
[32m+[m[32muse App\Models\Contact;[m
[32m+[m
[32m+[m[32mclass MapWidget extends Widget[m
[32m+[m[32m{[m
[32m+[m[32m    protected static string $view = 'filament.widgets.map-widget';[m
[32m+[m
[32m+[m[32m    protected static bool $isLazy = false;[m
[32m+[m
[32m+[m[32m    protected int | string | array $columnSpan = 'full';[m
[32m+[m
[32m+[m[32m    public $latitude;[m
[32m+[m[32m    public $longitude;[m
[32m+[m[32m    public $zoom = 13;[m
[32m+[m
[32m+[m[32m    public function mount(){[m
[32m+[m[32m        $contact = Contact::first();[m
[32m+[m
[32m+[m[32m        $this->latitude = $contact?->latitude ?? -6.9175;[m
[32m+[m[32m        $this->longitude = $contact?->longitude ?? 107.6191;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    protected function getViewData(): array[m
[32m+[m[32m    {[m
[32m+[m[32m        return [[m
[32m+[m[32m            'latitude' => $this->latitude,[m
[32m+[m[32m            'longitude' => $this->longitude,[m
[32m+[m[32m            'zoom' => $this->zoom,[m
[32m+[m[32m        ];[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
