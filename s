[32m+[m[32m  <script>[m
[32m+[m[32m    var swiper = new Swiper(".mySwiper", {[m
[32m+[m[32m      slidesPerView: 1,[m
[32m+[m[32m      spaceBetween: 30,[m
[32m+[m[32m      freeMode: true,[m
[32m+[m[32m      pagination: {[m
[32m+[m[32m        el: ".swiper-pagination",[m
[32m+[m[32m        clickable: true,[m
[32m+[m[32m      },[m
[32m+[m[32m      breakpoints: {[m
[32m+[m[32m        576: {[m
[32m+[m[32m          slidesPerView: 2[m
[32m+[m[32m        },[m
[32m+[m[32m        992: {[m
[32m+[m[32m          slidesPerView: 3[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m[32m  </script>[m
[32m+[m[32m  <script>[m
[32m+[m[32m    function setEqualCardHeights() {[m
[32m+[m[32m      const cards = document.querySelectorAll('.swiper-slide .card');[m
[32m+[m[32m      let maxHeight = 0;[m
[32m+[m
[32m+[m[32m      // Reset height dulu agar bisa hitung ulang kalau resize[m
[32m+[m[32m      cards.forEach(card => {[m
[32m+[m[32m        card.style.height = 'auto';[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      // Cari tinggi tertinggi[m
[32m+[m[32m      cards.forEach(card => {[m
[32m+[m[32m        if (card.offsetHeight > maxHeight) {[m
[32m+[m[32m          maxHeight = card.offsetHeight;[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      // Set semua card ke tinggi yang sama[m
[32m+[m[32m      cards.forEach(card => {[m
[32m+[m[32m        card.style.height = maxHeight + 'px';[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Jalankan saat halaman selesai dimuat[m
[32m+[m[32m    window.addEventListener('load', setEqualCardHeights);[m
[32m+[m[32m    // Jalankan lagi saat resize window[m
[32m+[m[32m    window.addEventListener('resize', setEqualCardHeights);[m
[32m+[m[32m  </script>[m
[32m+[m[32m</body>[m
[32m+[m
[32m+[m[32m</html>[m
