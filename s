[32m+[m[32m            'port' => env('MAIL_PORT', 2525),[m
[32m+[m[32m            'username' => env('MAIL_USERNAME'),[m
[32m+[m[32m            'password' => env('MAIL_PASSWORD'),[m
[32m+[m[32m            'timeout' => null,[m
[32m+[m[32m            'local_domain' => env('MAIL_EHLO_DOMAIN', parse_url(env('APP_URL', 'http://localhost'), PHP_URL_HOST)),[m
[32m+[m[32m        ],[m
[32m+[m[32m        'ses' => [[m
[32m+[m[32m            'transport' => 'ses',[m
[32m+[m[32m        ],[m
[32m+[m
[32m+[m[32m        'postmark' => [[m
[32m+[m[32m            'transport' => 'postmark',[m
[32m+[m[32m            // 'message_stream_id' => env('POSTMARK_MESSAGE_STREAM_ID'),[m
[32m+[m[32m            // 'client' => [[m
[32m+[m[32m            //     'timeout' => 5,[m
[32m+[m[32m            // ],[m
[32m+[m[32m        ],[m
[32m+[m
[32m+[m[32m        'resend' => [[m
[32m+[m[32m            'transport' => 'resend',[m
[32m+[m[32m        ],[m
[32m+[m
