[32m+[m[32muse Filament\Forms\Components\TextInput;[m
[32m+[m[32muse Filament\Forms\Components\Toggle;[m
[32m+[m[32muse Filament\Forms\Form;[m
[32m+[m[32muse Filament\Resources\Resource;[m
[32m+[m[32muse Filament\Tables;[m
[32m+[m[32muse Filament\Tables\Columns\IconColumn;[m
[32m+[m[32muse Filament\Tables\Columns\ImageColumn;[m
[32m+[m[32muse Filament\Tables\Columns\TextColumn;[m
[32m+[m[32muse Filament\Tables\Table;[m
[32m+[m[32muse Illuminate\Contracts\View\View;[m
[32m+[m[32muse Illuminate\Database\Eloquent\Builder;[m
[32m+[m[32muse Illuminate\Support\Str;[m
[32m+[m
[32m+[m[32mclass NewsResource extends Resource[m
[32m+[m[32m{[m
[32m+[m[32m    protected static ?string $model = News::class;[m
[32m+[m
[32m+[m[32m    protected static ?string $navigationIcon = 'heroicon-o-newspaper';[m
[32m+[m
[32m+[m[32m    public static function form(Form $form): Form[m
[32m+[m[32m    {[m
[32m+[m[32m        return $form[m
[32m+[m[32m            ->schema([[m
[32m+[m[32m                TextInput::make('title')[m
[32m+[m[32m                    ->label('Title berita')[m
[32m+[m[32m                    ->required()[m
[32m+[m[32m                    ->live(onBlur: true)[m
[32m+[m[32m                    ->afterStateUpdated(fn(string $state, Forms\Set $set) =>[m
[32m+[m[32m                    $set('slug', Str::slug($state))),[m
[32m+[m[32m                TextInput::make('slug')[m
[32m+[m[32m                    ->label('url')[m
[32m+[m[32m                    ->required(),[m
[32m+[m[32m                FileUpload::make('image')[m
[32m+[m[32m                    ->image()[m
[32m+[m[32m                    ->disk('public')[m
[32m+[m[32m                    ->directory('news')[m
[32m+[m[32m                    ->preserveFilenames()[m
[32m+[m[32m                    ->columnSpanFull(),[m
[32m+[m[32m                RichEditor::make('description')[m
[32m+[m[32m                    ->label('Description news')[m
[32m+[m[32m                    ->required()[m
[32m+[m[32m                    ->columnSpanFull(),[m
[32m+[m[32m                Toggle::make('is_published')[m
[32m+[m[32m                    ->label('Published')[m
[32m+[m[32m                    ->default(false)[m
[32m+[m[32m                    ->live()[m
[32m+[m[32m                    ->columnSpanFull()[m
[32m+[m[32m                    ->afterStateUpdated(function ($state, Forms\Set $set) {[m
[32m+[m[32m                        if ($state) {[m
[32m+[m[32m                            $set('published_at', now()->format('Y-m-d H:i:s'));[m
[32m+[m[32m                        } else {[m
[32m+[m[32m                            $set('published_at', null);[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }),[m
[32m+[m[32m                DateTimePicker::make('published_at')[m
[32m+[m[32m                    ->label('Publish Date')[m
[32m+[m[32m                    ->format('Y-m-d H:i:s')[m
[32m+[m[32m                    ->displayFormat('Y-m-d H:i:s')[m
[32m+[m[32m                    ->hidden(fn(Forms\Get $get): bool => !$get('is_published'))[m
[32m+[m[32m                    ->required(fn(Forms\Get $get): bool => $get('is_published'))[m
[32m+[m[32m            ]);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static function table(Table $table): Table[m
[32m+[m[32m    {[m
[32m+[m[32m        return $table[m
[32m+[m[32m            ->columns([[m
[32m+[m[32m                TextColumn::make('title')[m
[32m+[m[32m                    ->searchable()[m
[32m+[m[32m                    ->sortable(),[m
[32m+[m[32m                ImageColumn::make('image')[m
[32m+[m[32m                    ->square()[m
[32m+[m[32m                    ->sortable()[m
[32m+[m[32m                    ->searchable()[m
[32m+[m[32m                    ->size(70),[m
[32m+[m[32m                TextColumn::make('description')[m
[32m+[m[32m                    ->label('Description berita')[m
[32m+[m[32m                    ->sortable()[m
[32m+[m[32m                    ->searchable()[m
[32m+[m[32m                    ->formatStateUsing(fn($state) => strip_tags($state))[m
[32m+[m[32m                    ->limit(50)[m
[32m+[m[32m                    ->wrap(),[m
[32m+[m[32m                IconColumn::make('is_published')[m
[32m+[m[32m                    ->label('status')[m
[32m+[m[32m                    ->boolean()[m
[32m+[m[32m                    ->sortable(),[m
[32m+[m[32m                TextColumn::make('created_at')[m
[32m+[m[32m                    ->label('date created')[m
[32m+[m[32m                    ->dateTime()[m
[32m+[m[32m                    ->sortable()[m
[32m+[m[32m            ])[m
[32m+[m[32m            ->filters([[m
[32m+[m[32m                Tables\Filters\SelectFilter::make('is_published')[m
[32m+[m[32m                    ->options([[m
[32m+[m[32m                        '1' => 'Published',[m
[32m+[m[32m                        '0' => 'No Published',[m
[32m+[m[32m                    ]),[m
[32m+[m[32m                Tables\Filters\Filter::make('published_at')[m
[32m+[m[32m                    ->form([[m
[32m+[m[32m                        Forms\Components\DatePicker::make('date_created_start'),[m
[32m+[m[32m                        Forms\Components\DatePicker::make('date_created_end'),[m
[32m+[m[32m                    ])[m
[32m+[m[32m                    ->query(function (Builder $query, array $data): Builder {[m
[32m+[m[32m                        return $query[m
[32m+[m[32m                            ->when([m
[32m+[m[32m                                $data['date_created_start'],[m
[32m+[m[32m                                fn(Builder $query, $date): Builder => $query->whereDate('published_at', '>=', $date),[m
[32m+[m[32m                            )[m
[32m+[m[32m                            ->when([m
[32m+[m[32m                                $data['date_created_end'],[m
[32m+[m[32m                                fn(Builder $query, $date): Builder => $query->whereDate('published_at', '<=', $date),[m
[32m+[m[32m                            );[m
[32m+[m[32m                    })[m
[32m+[m[32m            ])[m
[32m+[m[32m            ->actions([[m
[32m+[m[32m                Tables\Actions\EditAction::make()[m
[32m+[m[32m                    ->color('warning'),[m
[32m+[m[32m                Tables\Actions\DeleteAction::make(),[m
[32m+[m[32m                Tables\Actions\Action::make('View description')[m
[32m+[m[32m                    ->icon('heroicon-o-eye')[m
[32m+[m[32m                    ->color('info')[m
[32m+[m[32m                    ->modalSubmitAction(false)[m
[32m+[m[32m                    ->modalCloseButton()[m
[32m+[m[32m                    ->modalContent(function (News $record): View {[m
[32m+[m[32m                        return view('filament.actions.view-description', [[m
[32m+[m[32m                            'record' => $record[m
[32m+[m[32m                        ]);[m
[32m+[m[32m                    })[m
[32m+[m[32m                    ->modalWidth('4xl'),[m
[32m+[m[32m            ])[m
[32m+[m[32m            ->bulkActions([[m
[32m+[m[32m                Tables\Actions\BulkActionGroup::make([[m
[32m+[m[32m                    Tables\Actions\DeleteBulkAction::make(),[m
[32m+[m[32m                ]),[m
[32m+[m[32m            ]);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static function getRelations(): array[m
[32m+[m[32m    {[m
[32m+[m[32m        return [[m
[32m+[m[32m            //[m
[32m+[m[32m        ];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static function getPages(): array[m
[32m+[m[32m    {[m
[32m+[m[32m        return [[m
[32m+[m[32m            'index' => Pages\ListNews::route('/'),[m
[32m+[m[32m            // 'create' => Pages\CreateNews::route('/create'),[m
[32m+[m[32m            // 'edit' => Pages\EditNews::route('/{record}/edit'),[m
[32m+[m[32m        ];[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
