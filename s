[32m+[m[32m                    ->label('Tanggal Diunggah')[m
[32m+[m[32m                    ->dateTime('d M Y, H:i')[m
[32m+[m[32m                    ->sortable()[m
[32m+[m[32m                    ->searchable(),[m
[32m+[m[32m                TextColumn::make('type')[m
[32m+[m[32m                    ->label('type gallery')[m
[32m+[m[32m                    ->sortable()[m
[32m+[m[32m                    ->searchable(),[m
[32m+[m[32m            ])[m
[32m+[m[32m            ->filters([[m
[32m+[m[32m                //[m
[32m+[m[32m            ])[m
[32m+[m[32m            ->actions([[m
[32m+[m[32m                Tables\Actions\EditAction::make()[m
