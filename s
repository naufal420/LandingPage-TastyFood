[1mdiff --git a/app/Services/LocationService.php b/app/Services/LocationService.php[m
[1mnew file mode 100644[m
[1mindex 0000000..9cbdfef[m
[1m--- /dev/null[m
[1m+++ b/app/Services/LocationService.php[m
[36m@@ -0,0 +1,51 @@[m
[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mnamespace App\Services;[m
[32m+[m
[32m+[m[32muse Illuminate\Support\Facades\Http;[m
[32m+[m[32muse Illuminate\Support\Facades\Log;[m
[32m+[m
[32m+[m[32mclass LocationService[m
[32m+[m[32m{[m
[32m+[m[32m  public function getCoordinates(string $location): array[m
[32m+[m[32m  {[m
[32m+[m[32m    // Default coordinates (Bandung)[m
[32m+[m[32m    $defaultCoordinates = [[m
[32m+[m[32m      'latitude' => -6.9175,[m
[32m+[m[32m      'longitude' => 107.6191,[m
[32m+[m[32m    ];[m
[32m+[m
[32m+[m[32m    try {[m
[32m+[m[32m      // Add user-agent as required by Nominatim usage policy[m
[32m+[m[32m      $response = Http::withHeaders([[m
[32m+[m[32m        'User-Agent' => 'Laravel/ContactApp'[m
[32m+[m[32m      ])->get('https://nominatim.openstreetmap.org/search', [[m
[32m+[m[32m        'q' => $location,[m
[32m+[m[32m        'format' => 'json',[m
[32m+[m[32m        'limit' => 1,[m
[32m+[m[32m      ]);[m
[32m+[m
[32m+[m[32m      // Check if response is successful and has data[m
[32m+[m[32m      if ($response->successful() && !empty($response->json())) {[m
[32m+[m[32m        $data = $response->json();[m
[32m+[m
[32m+[m[32m        // Check if we have results[m
[32m+[m[32m        if (isset($data[0]['lat']) && isset($data[0]['lon'])) {[m
[32m+[m[32m          return [[m
[32m+[m[32m            'latitude' => (float) $data[0]['lat'],[m
[32m+[m[32m            'longitude' => (float) $data[0]['lon'],[m
[32m+[m[32m          ];[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      // Return default coordinates if no results found[m
[32m+[m[32m      return $defaultCoordinates;[m
[32m+[m[32m    } catch (\Exception $e) {[m
[32m+[m[32m      // Log the error if needed[m
[32m+[m[32m      Log::error('Nominatim Geocoding Error: ' . $e->getMessage());[m
[32m+[m
[32m+[m[32m      // Return default coordinates on any error[m
[32m+[m[32m      return $defaultCoordinates;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
