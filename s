[32m+[m[32m    |[m
[32m+[m[32m    */[m
[32m+[m
[32m+[m[32m    'channels' => [[m
[32m+[m
[32m+[m[32m        'stack' => [[m
[32m+[m[32m            'driver' => 'stack',[m
[32m+[m[32m            'channels' => explode(',', env('LOG_STACK', 'single')),[m
[32m+[m[32m            'ignore_exceptions' => false,[m
[32m+[m[32m        ],[m
