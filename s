[36m@@ -0,0 +1,34 @@[m
[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mnamespace App\Models;[m
[32m+[m
[32m+[m[32muse Illuminate\Database\Eloquent\Factories\HasFactory;[m
[32m+[m[32muse Illuminate\Database\Eloquent\Model;[m
[32m+[m[32muse Illuminate\Support\Facades\Storage;[m
[32m+[m
[32m+[m[32mclass Gallery extends Model[m
[32m+[m[32m{[m
[32m+[m[32m    use HasFactory;[m
[32m+[m
[32m+[m[32m    protected $fillable = [[m
[32m+[m[32m        'title',[m
[32m+[m[32m        'image',[m
[32m+[m[32m        'type',[m
[32m+[m[32m        'timestamps',[m
[32m+[m[32m    ];[m
[32m+[m
[32m+[m[32m    protected static function boot()[m
[32m+[m[32m    {[m
[32m+[m[32m        parent::boot();[m
[32m+[m
[32m+[m[32m        static::creating(function ($gallery) {[m
[32m+[m[32m            $gallery->uploaded_at = now();[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        static::deleting(function ($gallery) {[m
[32m+[m[32m            if ($gallery->image) {[m
