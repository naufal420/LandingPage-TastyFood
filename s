[32m+[m[32m    |[m
[32m+[m[32m    */[m
[32m+[m
[32m+[m[32m    'from' => [[m
[32m+[m[32m        'address' => env('MAIL_FROM_ADDRESS', 'hello@example.com'),[m
[32m+[m[32m        'name' => env('MAIL_FROM_NAME', 'Laravel'),[m
[32m+[m[32m    ],[m
