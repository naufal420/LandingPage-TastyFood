[32m+[m[32m    {[m
[32m+[m[32m        return [[m
[32m+[m[32m            'index' => Pages\ListContactMessages::route('/'),[m
[32m+[m[32m            // 'create' => Pages\CreateContactMessage::route('/create'),[m
[32m+[m[32m            // 'edit' => Pages\EditContactMessage::route('/{record}/edit'),[m
[32m+[m[32m        ];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static function canCreate(): bool[m
[32m+[m[32m    {[m
[32m+[m[32m        return false;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static function getNavigationBadge(): ?string[m
[32m+[m[32m    {[m
[32m+[m[32m        return ContactMessage::where('is_read', false)->count() ?: null;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static function getNavigationBadgeColor(): ?string[m
[32m+[m[32m    {[m
[32m+[m[32m        return 'danger';[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
