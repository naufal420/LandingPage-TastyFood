[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mnamespace App\Models;[m
[32m+[m
[32m+[m[32muse Illuminate\Database\Eloquent\Factories\HasFactory;[m
[32m+[m[32muse Illuminate\Database\Eloquent\Model;[m
[32m+[m[32muse Illuminate\Support\Facades\Storage;[m
[32m+[m[32muse Illuminate\Support\Str;[m
[32m+[m
[32m+[m[32mclass News extends Model[m
[32m+[m[32m{[m
[32m+[m[32m    use HasFactory;[m
[32m+[m
[32m+[m[32m    protected $fillable = [[m
[32m+[m[32m        'title',[m
[32m+[m[32m        'slug',[m
[32m+[m[32m        'description',[m
[32m+[m[32m        'image',[m
[32m+[m[32m        'is_published',[m
[32m+[m[32m        'published_at',[m
[32m+[m[32m    ];[m
[32m+[m
[32m+[m[32m    protected $casts = [[m
[32m+[m[32m        'published_at' => 'datetime',[m
[32m+[m[32m        'is_published' => 'boolean',[m
[32m+[m[32m    ];[m
[32m+[m
[32m+[m[32m    protected static function boot()[m
[32m+[m[32m    {[m
[32m+[m[32m        parent::boot();[m
[32m+[m[32m        static::creating(function ($news) {[m
[32m+[m[32m            $news->slug = Str::slug($news->title);[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        static::saving(function ($news) {[m
[32m+[m[32m            if ($news->is_published && empty($news->published_at)) {[m
[32m+[m[32m                $news->published_at = now();[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            if (!$news->is_published) {[m
[32m+[m[32m                $news->published_at = null;[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        static::deleting(function ($news) {[m
[32m+[m[32m            if ($news->image) {[m
[32m+[m[32m                Storage::disk('public')->delete($news->image);[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
