[1mdiff --git a/app/Filament/Widgets/widgetsTotal.php b/app/Filament/Widgets/widgetsTotal.php[m
[1mnew file mode 100644[m
[1mindex 0000000..7c51161[m
[1m--- /dev/null[m
[1m+++ b/app/Filament/Widgets/widgetsTotal.php[m
[36m@@ -0,0 +1,145 @@[m
[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mnamespace App\Filament\Widgets;[m
[32m+[m
[32m+[m[32muse App\Models\ContactMessage;[m
[32m+[m[32muse App\Models\Gallery;[m
[32m+[m[32muse App\Models\News;[m
[32m+[m[32muse App\Models\testimonials;[m
[32m+[m[32muse Filament\Widgets\StatsOverviewWidget as BaseWidget;[m
[32m+[m[32muse Filament\Widgets\StatsOverviewWidget\Stat;[m
[32m+[m[32muse Filament\Forms\Components\DatePicker;[m
[32m+[m[32muse Illuminate\Support\Carbon;[m
[32m+[m[32muse Filament\Forms;[m
[32m+[m[32muse Livewire\Attributes\On;[m
[32m+[m
[32m+[m[32mclass widgetsTotal extends BaseWidget[m
[32m+[m[32m{[m
[32m+[m[32m    protected static ?string $pollingInterval = null;[m
[32m+[m
[32m+[m[32m    // Add these properties for the date filtering[m
[32m+[m[32m    public ?string $startDate = null;[m
[32m+[m[32m    public ?string $endDate = null;[m
[32m+[m
[32m+[m[32m    public function mount(): void[m
[32m+[m[32m    {[m
[32m+[m[32m        // Ambil tanggal pertama kali data dibuat dari tabel yang relevan[m
[32m+[m[32m        $earliestGalleryDate = Gallery::oldest('created_at')->value('created_at');[m
[32m+[m[32m        $earliestContactMessageDate = ContactMessage::oldest('created_at')->value('created_at');[m
[32m+[m[32m        $earliestNewsDate = News::oldest('created_at')->value('created_at');[m
[32m+[m[32m        $earliestTestimonialsDate = testimonials::oldest('created_at')->value('created_at');[m
[32m+[m[32m        // Temukan tanggal paling awal di antara semua tabel[m
[32m+[m[32m        $earliestDate = collect([$earliestGalleryDate, $earliestContactMessageDate, $earliestNewsDate, $earliestTestimonialsDate])[m
[32m+[m[32m            ->filter()[m
[32m+[m[32m            ->min();[m
[32m+[m
[32m+[m[32m        // Set startDate ke tanggal paling awal, atau default ke 1 bulan yang lalu jika tidak ada data[m
[32m+[m[32m        $this->startDate = $earliestDate ? Carbon::parse($earliestDate)->format('Y-m-d') : now()->subMonth()->format('Y-m-d');[m
[32m+[m
[32m+[m[32m        // Set endDate ke tanggal sekarang[m
[32m+[m[32m        $this->endDate = now()->format('Y-m-d');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Override the default view to include our custom date filters[m
[32m+[m[32m    protected static string $view = 'widgets.stats-overview-with-filters';[m
[32m+[m
[32m+[m[32m    // Add a filter method to be called when the filter button is clicked[m
[32m+[m[32m    public function filter(): void[m
[32m+[m[32m    {[m
[32m+[m[32m        $this->dispatch('filteredStatsOverview');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    #[On('filteredStatsOverview')][m
[32m+[m[32m    public function updateStats(): void[m
[32m+[m[32m    {[m
[32m+[m[32m        // This method will be called when the stats need to be updated[m
[32m+[m[32m        // The widget will re-render automatically[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    protected function getStats(): array[m
[32m+[m[32m    {[m
[32m+[m[32m        $startDate = $this->startDate ? Carbon::parse($this->startDate) : null;[m
[32m+[m[32m        $endDate = $this->endDate ? Carbon::parse($this->endDate) : null;[m
[32m+[m
[32m+[m[32m        return [[m
[32m+[m[32m            Stat::make('Gallery', $this->getGalleryCount($startDate, $endDate))[m
[32m+[m[32m                ->description('Total Gambar yang diunggah')[m
[32m+[m[32m                ->icon('heroicon-o-photo')[m
[32m+[m[32m                ->color('success')[m
[32m+[m[32m                ->url(route('filament.admin.resources.galleries.index')),[m
[32m+[m
[32m+[m[32m            Stat::make('Contact Message', $this->getContactMessageCount($startDate, $endDate))[m
[32m+[m[32m                ->description('Total message tersimpan')[m
[32m+[m[32m                ->icon('heroicon-o-chat-bubble-left')[m
[32m+[m[32m                ->color('success')[m
[32m+[m[32m                ->url(route('filament.admin.resources.contact-messages.index')),[m
[32m+[m
[32m+[m[32m            Stat::make('News', $this->getNewsCount($startDate, $endDate))[m
[32m+[m[32m                ->description('Total berita yang dibuat / diterbitkan')[m
[32m+[m[32m                ->icon('heroicon-o-newspaper')[m
[32m+[m[32m                ->color('success')[m
[32m+[m[32m                ->url(route('filament.admin.resources.news.index')),[m
[32m+[m[32m            Stat::make('testimonials', $this->getTestimonialsCount($startDate, $endDate))[m
[32m+[m[32m                ->description('Total ulasan tersimpan')[m
[32m+[m[32m                ->icon('heroicon-o-chat-bubble-left')[m
[32m+[m[32m                ->color('success')[m
[32m+[m[32m                ->url(route('filament.admin.resources.testimonials.index')),[m
[32m+[m[32m        ];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Helper methods to get filtered counts[m
[32m+[m[32m    private function getGalleryCount(?Carbon $startDate, ?Carbon $endDate): int[m
[32m+[m[32m    {[m
[32m+[m[32m        $query = Gallery::query();[m
[32m+[m
[32m+[m[32m        if ($startDate && $endDate) {[m
[32m+[m[32m            $query->whereBetween('created_at', [$startDate->startOfDay(), $endDate->endOfDay()]);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return $query->count();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private function getContactMessageCount(?Carbon $startDate, ?Carbon $endDate): int[m
[32m+[m[32m    {[m
[32m+[m[32m        $query = ContactMessage::query();[m
[32m+[m
[32m+[m[32m        if ($startDate && $endDate) {[m
[32m+[m[32m            $query->whereBetween('created_at', [$startDate->startOfDay(), $endDate->endOfDay()]);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return $query->count();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private function getNewsCount(?Carbon $startDate, ?Carbon $endDate): int[m
[32m+[m[32m    {[m
[32m+[m[32m        $query = News::query();[m
[32m+[m
[32m+[m[32m        if ($startDate && $endDate) {[m
[32m+[m[32m            $query->whereBetween('created_at', [$startDate->startOfDay(), $endDate->endOfDay()]);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return $query->count();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private function getTestimonialsCount(?Carbon $startDate, ?Carbon $endDate): int[m
[32m+[m[32m    {[m
[32m+[m[32m        $query = testimonials::query();[m
[32m+[m
[32m+[m[32m        if ($startDate && $endDate) {[m
[32m+[m[32m            $query->whereBetween('created_at', [$startDate->startOfDay(), $endDate->endOfDay()]);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return $query->count();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Helper to create a date range description[m
[32m+[m[32m    private function getDateRangeDescription(): string[m
[32m+[m[32m    {[m
[32m+[m[32m        if (!$this->startDate || !$this->endDate) {[m
[32m+[m[32m            return '';[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return ' (' . Carbon::parse($this->startDate)->format('d M Y') . ' - ' .[m
[32m+[m[32m            Carbon::parse($this->endDate)->format('d M Y') . ')';[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
