[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mreturn [[m
[32m+[m
[32m+[m[32m    /*[m
[32m+[m[32m    |--------------------------------------------------------------------------[m
[32m+[m[32m    | Broadcasting[m
[32m+[m[32m    |--------------------------------------------------------------------------[m
[32m+[m[32m    |[m
[32m+[m[32m    | By uncommenting the Laravel Echo configuration, you may connect Filament[m
[32m+[m[32m    | to any Pusher-compatible websockets server.[m
[32m+[m[32m    |[m
[32m+[m[32m    | This will allow your users to receive real-time notifications.[m
[32m+[m[32m    |[m
[32m+[m[32m    */[m
